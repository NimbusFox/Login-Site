@model HtmlString
﻿@if (!HttpContext.Current.Request.Cookies.AllKeys.Any(x => x == "AcceptedCookies") && !string.IsNullOrEmpty(Model.ToString())) {
<div class="container" style="position: absolute; bottom: 20px" id="Cookies">
    <div class="alert alert-dismissible alert-info">
        <button class="btn btn-success" style="position: absolute !important; top: 10px; right: 20px" onclick="acceptCookies();"><span>&times;</span></button>
        @Model
    </div>
</div>
}
